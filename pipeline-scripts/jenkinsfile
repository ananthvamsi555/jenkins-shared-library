@Library('jenkins-shared-library') _
pipeline {
    agent any
    environment {
        repoUrl = "https://github.com/ananthvamsi555/jenkins-shared-library.git"
        branch = "main"
    }
    stages {
        stage('Checkout Code') {
            steps {
                script {
                    clone(repoUrl, branch)
                }
            }
        }
    }
}

